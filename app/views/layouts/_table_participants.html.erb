<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
    <thead>
        <tr>
            <th class="show-quadrante text-center" scope="col-6">Nome Completo <br> Endere√ßo </th>
            <th class="show-quadrante text-center" scope="col-2">Apelido <br> Email</th>
            <th class="show-quadrante text-center" scope="col-2">Nascimento <br> Contato</th>
        </tr>
    </thead>
    <tbody>
        <% participants.each do |p| %>
            <tr>
                <td class="show-quadrante text-center"><%= p.name%></td>
                <td class="show-quadrante text-center"><%= "--" if !p.surname.present? %><%= p.surname %></td>
                <td class="show-quadrante text-center"><%= mes_dia_ano_completo(p.birth) %></td>
            </tr>
                <tr>
                <td class="show-quadrante text-center"><%= p.address%></td>
                <td class="show-quadrante text-center"><%= "--" if !p.email.present? %><%= p.email %></td>
                <td class="show-quadrante text-center"><%= "--" if !p.contact.present? %><%= p.contact %></td>
            </tr>
            <tr style="height: 20px"></tr>
        <% end %>
    </tbody>
</table>